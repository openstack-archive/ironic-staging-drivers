{
    "title": "Intel Node Manager statistics schema",
    "type": "object",
    "properties": {
        "scope": {
            "type": "string",
            "enum": ["global", "policy"]
        },
        "parameter_name": {
            "type": "string",
            "enum": ["power", "temperature", "throttling", "airflow", "airflow_temperature", "chassis_power", "unhandled_requests", "response_time", "cpu_throttling", "memory_throttling", "communication_failures", "trigger"]
        },
        "domain_id": {
            "type": "string",
            "enum": ["platform", "cpu", "memory", "io", "protection"]
        },
        "policy_id": {
            "type": "integer",
            "minimum": 0,
            "maximum": 255
        }
    },
    "required": ["scope", "domain_id"],
    "additionalProperties": false
}
